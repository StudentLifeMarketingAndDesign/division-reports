{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.5.*@stable",
		"silverstripe/framework": "3.5.*@stable",
		"silverstripe/reports": "3.5.*@stable",
		"silverstripe/siteconfig": "3.5.*@stable",
		"silverstripe-themes/simple": "3.1.*",
		"silverstripe/blog" : "2.*@stable",
		"silverstripe/activedirectory": "3.*",
		"studiobonito/silverstripe-typekit" : "*",
		"sheadawson/silverstripe-blocks": "1.1.1",
		"silverstripe-australia/gridfieldextensions":"1.4.*",
		"nathancox/codeeditorfield" : "1.3.*",
		"md/uiowa-bar": "*",
		"unclecheese/display-logic" : "*",
		"jonom/focuspoint":"2.4.*"

	},
   "repositories":[  
      {
         "type": "vcs",
         "url": "https://github.com/StudentLifeMarketingAndDesign/silverstripe-activedirectory"
      },
      {
         "type": "vcs",
         "url": "https://github.com/StudentLifeMarketingAndDesign/silverstripe-uiowa-bar"
      }
   ],
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.5.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
